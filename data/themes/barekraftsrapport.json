  "executive_summary": {
    "page": 2,
    "conclusion": "Rehabilitering av eksisterende bygg det mest bærekraftige alternativet",
    "key_findings": [
      {
        "category": "Klimaavtrykk",
        "finding": "Rehabilitering reduserer de totale klimagassutslippene med 48% sammenlignet med riving og nybygg"
      },
      {
        "category": "Materialbruk",
        "finding": "Bevaring av bygningskroppen reduserer behovet for nye materialer med over 85%"
      },
      {
        "category": "Avfall",
        "finding": "Rehabilitering reduserer avfallsmengden fra 800-1200 kg/m² til kun 100-200 kg/m²"
      },
      {
        "category": "Energieffektivitet",
        "finding": "Oppgradering kan redusere energibehovet med cirka 50%, og oppnå en standard på nivå med et TEK17-nybygg"
      },
      {
        "category": "Risikoreduksjon",
        "finding": "Riving unngås, som reduserer de geotekniske risikoene i et område utsatt for kvikkleireproblematikk"
      }
    ]
  },

